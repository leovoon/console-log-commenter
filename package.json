{
    "name": "console-log-commenter",
    "displayName": "Console Log Commenter",
    "description": "Quickly comment and uncomment all console.log statements in the current file via the context menu.",
    "version": "1.0.0",
    "publisher": "leovoon",
    "repository": {
      "type": "git",
      "url": "https://github.com/leovoon/console-log-commenter.git"
    },
    "engines": {
      "vscode": "^1.80.0"
    },
    "categories": [
      "Programming Languages",
      "Other"
    ],
    "activationEvents": [
      "onCommand:console-log-commenter.commentLogs",
      "onCommand:console-log-commenter.uncommentLogs"
    ],
    "main": "./extension.js",
    "contributes": {
      "commands": [
        {
          "command": "console-log-commenter.commentLogs",
          "title": "Comment out all console.log"
        },
        {
          "command": "console-log-commenter.uncommentLogs",
          "title": "Uncomment all console.log"
        }
      ],
      "menus": {
        "editor/context": [
          {
            "when": "editorTextFocus",
            "command": "console-log-commenter.commentLogs",
            "group": "modification@1"
          },
          {
            "when": "editorTextFocus",
            "command": "console-log-commenter.uncommentLogs",
            "group": "modification@2"
          }
        ]
      }
    },
    "scripts": {
      "lint": "eslint .",
      "pretest": "npm run lint",
      "test": "node ./test/runTest.js"
    },
    "devDependencies": {
      "eslint": "^9.35.0",
      "glob": "^11.0.3",
      "mocha": "^10.1.0",
      "@vscode/test-electron": "^2.2.0"
    }
  }
  